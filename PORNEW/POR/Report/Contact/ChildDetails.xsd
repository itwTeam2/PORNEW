                   pcd.BirthPlace,
                   pcd.DeathCertificateNo,
                   pcd.DateOfDeath,
                   pch.Active,
                   pch.CreatedBy,
                   pch.CreatedDate,
                   VPD.Branch,
                   VPD.Name,
                   VPD.ServiceNo,
                   VPD.Rank, 
				   pch.PreviousReject,
				   pch.RejectAuth

            FROM dbo.PsnContactHeader pch
                INNER JOIN dbo.PsnChildDetails pcd
                    ON pch.PCHID = pcd.PsnConHeaderID
                INNER JOIN dbo.PORMasterSubCategory pmsc
                    ON pmsc.MSCID = pch.MasSubCatID
                INNER JOIN Vw_PersonalDetail VPD
                    ON pch.Sno = VPD.SNo</CommandText>
